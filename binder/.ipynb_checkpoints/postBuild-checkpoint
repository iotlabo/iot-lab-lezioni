#!/bin/bash

# Install a JupyterLab extension for demonstration purposes

jupyter labextension install --no-build @jupyterlab/celltags @jupyterlab/git @jupyterlab/github @jupyterlab/plotly-extension @jupyterlab/statusbar @jupyterlab/toc @mflevine/jupyterlab_html @quentinandre/jupyterlab_snippets @rmotr/jupyterlab-solutions jupyterlab_bokeh jupyterlab-python-file @enlznep/runall-extension @krassowski/jupyterlab_go_to_definition @lckr/jupyterlab_variableinspector @parente/jupyterlab-quickopen @pyviz/jupyterlab_pyviz @jupyter-widgets/jupyterlab-manager bqplot jupyter-matplotlib jupyterlab-datawidgets jupyter-leaflet jupyterlab-topbar-extension jupyterlab-system-monitor 

jupyter lab build

jupyter serverextension enable --py jupyterlab_quickopen --user
jupyter serverextension enable --py jupyterlab_git
jupyter nbextension enable --py --sys-prefix pythreejs